{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["jest"],
    "overrides": [
      {
        "files":["tests/*.js"],
        "rules": {
          "no-unused-vars":"off"
        }
      }
    ]
}
